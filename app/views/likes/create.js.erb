var numLikes = parseInt($("#<%=@like.likeable_type%><%=@like.likeable_id%> .vote-button .num-likes").text(), 10) + 1

$("#<%=@like.likeable_type%><%=@like.likeable_id%> .vote-button").html('<%= j (render "remove_like", path: eval("likes_#{@like.likeable_type.downcase}_path(@like.likeable_id)"), num_likes: 0) %>');
$(function () {
  $('[data-toggle="tooltip"]').tooltip()
  $("#<%=@like.likeable_type%><%=@like.likeable_id%> .vote-button .num-likes").text(numLikes);
})



